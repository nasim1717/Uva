#include<bits/stdc++.h>
using namespace std;
main(){
    int a,b,c;
    while(scanf("%d %d %d", &a,&b,&c)!=EOF){
        if((a==0 & b==0 && c==0) || (a==1 && b==1 && c==1))
            cout<< "*\n";
        else if(a==0 && b==1 && c==1)
            cout<<"A\n";
        else if(a==1 & b==0 && c==0)
            cout<<"A\n";
        else if(a==0 && b==1 && c==0)
            cout<<"B\n";
        else if(a==1 && b==0 && c==1)
            cout<<"B\n";
        else if(a==1 && b==1 && c==0)
            cout<<"C\n";
        else if(a==0 && b==0 && c==1)
            cout<<"C\n";

    }
return 0;
}
