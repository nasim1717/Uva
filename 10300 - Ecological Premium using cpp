#include<bits/stdc++.h>
using namespace std;
main(){
    int t,a,b,c,n,i,j,sum;
    while(scanf("%d", &t)!=EOF){
        for(i=0; i<t; i++){
            cin>>n;
            sum=0;
        for(j=0; j<n; j++){
            cin>>a>>b>>c;
            sum+=a*c;
        }
        printf("%d\n", sum);
    }

    }
return 0;
}
