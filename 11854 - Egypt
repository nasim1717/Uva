
#include<bits/stdc++.h>
using namespace std;
int main(){
   long long int a,b,c,sq1,sq2,sq3;
    while(1){
        cin>>a>>b>>c;
        if(a==0 && b==0 && c==0)
            break;
        sq1 = a*a;
        sq2 = b*b;
        sq3 = c*c;
        if(sq1+sq2==sq3 || sq2+sq3 == sq1 || sq3+sq1==sq2)
            cout<<"right"<<endl;
        else
            cout<<"wrong"<<endl;

    }
return 0;
}
