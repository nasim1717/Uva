
#include<bits/stdc++.h>
using namespace std;
main(){
    int t,n,sum=0,cas=0;
    while(1){
        cin>>t;
        cas++;
        if(t==0)
            break;
        while(t--){
            cin>>n;
            if(n>0)
                sum++;
            else
                sum--;
        }
        printf("Case %d: %d\n",cas,sum);
        sum=0;
    }
return 0;
}
