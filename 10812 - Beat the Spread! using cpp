#include<bits/stdc++.h>
using namespace std;
int main(){
    int t,a,b,x,y;
    while(scanf("%d", &t)!=EOF){
        while(t--){
        cin>>a>>b;
        if(a>=b && (a+b)%2==0){
            x = (a+b)/2;
            y = (a-b)/2;
            cout<<x<<" "<<y<<endl;
        }
        else
            printf("impossible\n");
        }
    }
return 0;
}
