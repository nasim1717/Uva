#include<bits/stdc++.h>
using namespace std;
int main(){
    int tc,i,j,flag,sbt;
    string text,subtext;
    cin>>tc;
    while(tc--){
        cin>>text;
        cin>>sbt;
        for(j=0; j<sbt; j++){
        flag=0;
        cin>>subtext;
        for(i=0; i<subtext.size(); i++){
            if(text[i]!=subtext[i])
                flag=1;
        }
        if(flag==0)
            printf("y\n");
        else
            printf("n\n");
        }
    }
return 0;
}
