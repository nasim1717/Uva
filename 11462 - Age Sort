#include<bits/stdc++.h>
using namespace std;
int main(){
    int t,i,n;
    while(cin>>t and t){
        vector<long long int>v;
       for(i=0;i<t; i++){
            cin>>n;
            v.push_back(n);
       }
       sort(v.begin(),v.end());
       int x = v.size();
        for(i=0; i<x-1; i++)
            cout<<v[i]<<" ";
            cout<<v[x-1]<<endl;
    }
return 0;
}
