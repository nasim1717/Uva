
#include<bits/stdc++.h>
using namespace std;

int main(){
    int x,div,mid;
    vector<int>v;
    while(cin>>x){
        v.push_back(x);
        sort(v.begin(),v.end());
        if(v.size()%2!=0){
            div=(v.size()+1)/2;
            cout<<v[div-1]<<endl;
        }
        else{
            div=(v.size()+1)/2;
            mid=(v[div-1]+v[div])/2;
            cout<<mid<<endl;
        }
    }
return 0;
}
