// problem no 11044

#include<stdio.h>
main(){
    int t,cl,row,n,m,sy,sy1;
    scanf("%d", &t);
    while(t--){
        scanf("%d %d", &n,&m);
        row = n-2;
        cl = m-2;
        sy = row/3;
        sy1 = cl/3;
        if(row%3!=0)
            sy++;
        if(cl%3!=0)
            sy1++;
        printf("%d\n", sy*sy1);
    }

return 0;
}
