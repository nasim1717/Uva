//problem no 11172

#include<stdio.h>
main(){
    int t;
    long long int a,b;
    scanf("%d", &t);
    while(t--){
        scanf("%lld %lld", &a,&b);
        if(a>b)
            printf(">\n");
        else if(a<b)
            printf("<\n");
        else
            printf("=\n");
    }
    return 0;
}
