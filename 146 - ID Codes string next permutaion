
#include<bits/stdc++.h>
using namespace std;
int main(){
    string st;
    while(cin>>st){
        if(st=="#")
            break;
        else{
            if(next_permutation(st.begin(), st.end()))
                cout<<st<<endl;
            else
                cout<<"No Successor"<<endl;
        }
    }
return 0;
}

