#include<bits/stdc++.h>
using namespace std;
int main(){
    int i,j,G,n;
    while(cin>>n){
        if(n==0)
            break;
        G=0;
        for(i=1; i<n; i++){
            for(j=i+1; j<=n; j++){
                G+= __gcd(i,j);
            }

        }
        cout<<G<<endl;
    }
return 0;
}
